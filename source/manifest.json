{
	"name": "Uniform Timezone Extension",
	"version": "0.0.5",
	"description": "Brings standardization to social media posts' dates and times.",
	"homepage_url": "https://github.com/bellingcat/uniform-timezone",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"browser_specific_settings": {
		"gecko": {
			"id": "todo@github.io",
			"strict_min_version": "100.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage", "tabs"
	],
	"host_permissions": [],
	"content_scripts": [{
		"matches": ["https://twitter.com/*", "https://web.archive.org/web/*/https://twitter.com/*"],
		"js": ["js/timezone-fixers/twitter.js"],
		"css": ["css/hover-popup.css"],
		"run_at": "document_end"
	}, {
		"matches": ["https://*.tiktok.com/*", "https://web.archive.org/web/*/https://*.tiktok.com/*"],
		"js": ["js/timezone-fixers/tiktok.js"],
		"css": ["css/hover-popup.css"],
		"run_at": "document_end"
	}, {
		"matches": ["https://discord.com/*", "https://web.archive.org/web/*/https://discord.com/*"],
		"js": ["js/timezone-fixers/discord.js"],
		"css": ["css/hover-popup.css"],
		"run_at": "document_end"
	}],
	"action": {
		"default_popup": "html/popup.html"
	},
	"background": {
		"service_worker": "js/background.js",
		"type": "module"
	},
	"web_accessible_resources": [{
		"resources": ["img/icon-128.png"],
		"matches": ["https://twitter.com/*", "https://*.tiktok.com/*", "https://web.archive.org/*", "https://discord.com/*"]
	}],
	"key": ""
}
